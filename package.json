{
    "name": "filemaker-vscode",
    "displayName": "FileMaker VSCode",
    "description": "FileMaker syntax highlighting and snippets for Visual Studio Code",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "filemaker",
            "aliases": ["FileMaker", "Filemaker", "filemaker"],
            "extensions": [".fmfn",".calc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "filemaker",
            "scopeName": "source.filemaker",
            "path": "./syntaxes/FileMaker.tmLanguage"
        }],
        "snippets": [{
            "language": "filemaker",
            "path": "./snippets/filemaker.json"    
        }]
    }
}